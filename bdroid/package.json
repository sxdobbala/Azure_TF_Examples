{
  "name": "bdroid",
  "version": "1.0.0",
  "description": "Angular-NestJS Starter Kit Monorepo",
  "author": "Sumanth Chinthagunta <xmlking@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.optum.com/ees-osfi/bdroid.git"
  },
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test webapp",
    "test:ci": "ng test webapp --ci --max-workers=4",
    "test:affected": "npm run affected:test  -- --uncommitted",
    "lint": "./node_modules/.bin/nx lint && ng lint",
    "e2e": "ng e2e",
    "start:mock": "ng serve -c=mock",
    "prebuild": "rimraf dist coverage",
    "// - BUILDING": "Build Apps for Prod, Mock environments",
    "build:prod": "ng build -c=production",
    "build:mock": "ng build -c=mock --base-href /pages/ees-osfi/bdroid/",
    "deploy:mock": "npm run build:mock && ngh --dir dist/apps/webapp",
    "bundle-report": "ng build -c=production --stats-json && webpack-bundle-analyzer dist/apps/webapp/stats.json",
    "// - DOCS": "Generate and serve docs",
    "doc:build": "compodoc -p tsconfig.json -d docs",
    "doc:serve": "compodoc -s -d docs",
    "doc:build-serve": "compodoc -p tsconfig.json -d docs -s",
    "// - AFFECTED": "work on only affected modules",
    "affected:apps": "./node_modules/.bin/nx affected:apps",
    "affected:libs": "./node_modules/.bin/nx affected:libs",
    "affected:build": "./node_modules/.bin/nx affected:build",
    "affected:e2e": "./node_modules/.bin/nx affected:e2e",
    "affected:test": "./node_modules/.bin/nx affected:test",
    "affected:lint": "./node_modules/.bin/nx affected:lint",
    "affected:dep-graph": "./node_modules/.bin/nx affected:dep-graph",
    "// - FORMAT": "Format and lint Utils",
    "format": "./node_modules/.bin/nx format:write",
    "format:write": "./node_modules/.bin/nx format:write",
    "format:check": "./node_modules/.bin/nx format:check",
    "update": "ng update @nrwl/schematics",
    "update:check": "ng update",
    "workspace-schematic": "./node_modules/.bin/nx workspace-schematic",
    "dep-graph": "./node_modules/.bin/nx dep-graph",
    "help": "./node_modules/.bin/nx help",
    "// - API Serve": "API Serve",
    "api:start": "ts-node -r tsconfig-paths/register apps/api/src/main.ts",
    "api:start:dev": "nodemon --config ./apps/api/nodemon.json",
    "api:start:debug": "nodemon --config ./apps/api/nodemon-debug.json",
    "api:prestart:prod": "rimraf dist && tsc -p ./apps/api/tsconfig.app.json",
    "api:start:prod": "node dist/apps/api/main.js",
    "// - API Test": "API Testing",
    "api:test": "jest --projects=apps/api --roots=src",
    "api:test:watch": "jest --projects=apps/api --roots=src --watch",
    "api:test:cov": "jest --projects=apps/api --coverage",
    "api:test:ci": "jest --projects=apps/api --run-in-band",
    "api:e2e": "jest --projects=apps/api-e2e --roots=src --forceExit",
    "api:e2e:watch": "jest --projects=apps/api-e2e --roots=src --watch"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^7.0.0",
    "@angular/cdk": "^7.0.0",
    "@angular/common": "^7.0.0",
    "@angular/compiler": "^7.0.0",
    "@angular/core": "^7.0.0",
    "@angular/flex-layout": "^7.0.0-beta.19",
    "@angular/forms": "^7.0.0",
