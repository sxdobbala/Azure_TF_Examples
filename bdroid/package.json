    "ngx-moment": "^3.2.0",
    "ngx-page-scroll": "^5.0.0",
    "ngx-perfect-scrollbar": "^7.0.0",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0",
    "pg": "^7.6.1",
    "rxjs": "^6.3.3",
    "screenfull": "^3.3.3",
    "smooth-scrollbar": "^8.3.1",
    "typeorm": "^0.2.8",
    "underscore": "^1.9.1",
    "uuid": "^3.3.2",
    "uuid-parse": "^1.0.0",
    "zone.js": "^0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.10.0",
    "@angular-devkit/build-ng-packagr": "^0.10.0",
    "@angular/cli": "^7.0.0",
    "@angular/compiler-cli": "^7.0.0",
    "@angular/http": "^7.0.0",
    "@angular/language-service": "^7.0.0",
    "@compodoc/compodoc": "^1.1.6",
    "@nestjs/testing": "^5.4.0",
    "@ngx-formly/schematics": "5.0.0-beta.5",
